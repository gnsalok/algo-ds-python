{
    //Python 
    "python.formatting.provider": "autopep8",

    //CodeRunner
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ -std=c++11 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
    },
    "java.project.sourcePaths": [
        "algo-ds-java/learning_java",
        "algo-ds-java/learning-java",
        "algo-ds-java/Array",
        "algo-ds-java/leetcode-java/javanotes"
    ],
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "python": false,
        "go": false
    }
}

